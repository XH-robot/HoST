<mujoco model="ths_23dof">

    <!-- 编译器配置  角度单位：弧    网格文件搜索路径      -->
    <compiler angle="radian"     meshdir="../meshes/"  />
    <!-- 仿真资源容量配置 最大关节数限制 最大接触对数限制-->
    <size njmax="500" nconmax="100" />


    <!-- 全局仿真参数配置 仿真时间步长        每个步内执行的迭代次数  约束求解算法    重力加速度-->
    <option            timestep='0.001'  iterations='50'      solver='PGS'  gravity='0 0 -9.81'>
    <!-- 功能开关标志 启用接触检测       启用能量守恒计算   启用重力模拟-->
    <flag           contact="enable" energy="enable" gravity="enable"/>

    </option>


    <!-- 视觉效果全局配置 -->
    <visual>
        <quality shadowsize='4096'/>  <!-- 阴影质量设置 阴影贴图分辨率（4096x4096像素） -->
        <map znear='0.05'/>           <!-- 摄像机渲染范围 近裁剪平面距离 -->
    </visual>
    

    <!-- 阻尼、摩擦力、惯量等 -->
    <default>
        <default class="torso_motor">    <!-- 躯体电机 -->
            <joint damping="0.05" armature="0.01" frictionloss="0.2"/>
        </default>
        <default class="leg_motor">      <!-- 腿部电机电机 -->
            <joint damping="0.05" armature="0.01" frictionloss="0.2"/>
        </default>
        <default class="ankle_motor">    <!-- 脚踝电机 -->
            <joint damping="0.05" armature="0.01" frictionloss="0.2"/>
        </default>
        <default class="arm_motor">      <!-- 手臂u电机 -->
            <joint damping="0.05" armature="0.01" frictionloss="0.2"/>
        </default>
        <default class="wrist_motor">    <!-- 手腕电机 -->
            <joint damping="0.05" armature="0.01" frictionloss="0.1"/>
        </default>
        
    </default>   


    <asset>



        <!--    纹理类型：天空盒  使用内置渐变生成器    纹理宽度     纹理高度       渐变起始颜色（蓝灰色） 渐变结束颜色（纯黑）  -->
        <texture type='skybox' builtin='gradient' width='128' height='128' rgb1='0.4 0.6 0.8' rgb2='0.4 0.6 0.8'/>
        <!--     2D平面纹理 棋盘格纹理名称  内置棋盘格图案      叠加十字标记   棋盘格浅色块颜色 深蓝灰  棋盘格深色块颜色 炭灰色  十字标记标记颜色          纹理（宽）   纹理（高） -->
        <texture type="2d" name="plane" builtin="checker" mark="cross" rgb1="0.2 0.3 0.4"   rgb2="0.1 0.15 0.2"   markrgb="0.8 0.8 0.8"  width="512" height="512" />
        <!--     材质名称       反射率0无反射        关联的2D纹理名称  纹理平铺次数（X/Y轴各1次  禁用UV坐标缩放（保持纹理比例）  -->
        <material name='plane' reflectance='0.' texture='plane' texrepeat='1 1'        texuniform='true'/>
        <!-- 碰撞体材质名称           RGBA颜色 -->
        <material name='collision' rgba='0.1 0.5 0.9 0'/> <!--collision rgba 无颜色-->



        <mesh name="base_link" file="base_link.STL" />
        <mesh name="left_hip_pitch_link" file="left_hip_pitch_link.STL" />
        <mesh name="left_hip_roll_link" file="left_hip_roll_link.STL" />
        <mesh name="left_hip_yaw_link" file="left_hip_yaw_link.STL" />
        <mesh name="left_knee_link" file="left_knee_link.STL" />
